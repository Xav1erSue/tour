    "guide",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Xav1erSue/tour.git"
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "typings": "es/index.d.ts",
